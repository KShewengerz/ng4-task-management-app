{
  "name": "task-management-system",
  "version": "0.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "build": "concurrently \"npm install\" \"cd public/app && npm install\"",
    "ng-start": "cd ./public/app && tsc && npm start",
    "express-start": "tsc-watch --onSuccess 'nodemon ./bin/www'",
    "start": "concurrently \"npm run express-start\"  \"npm run ng-start\"",
    "knex:latest": "babel-node --extensions '.ts' node_modules/.bin/knex migrate:latest",
    "knex:rollback": "babel-node --extensions '.ts' node_modules/.bin/knex migrate:rollback"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.45",
    "@types/express": "^4.11.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^7.0.60",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "mysql": "^2.15.0",
    "serve-favicon": "~2.4.2",
    "snakecase-keys": "^1.1.1",
    "ts-node": "^6.0.0",
    "typescript": "^2.8.1"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.8",
    "@babel/core": "^7.0.0-beta.45",
    "@babel/node": "^7.0.0-beta.45",
    "@babel/preset-env": "^7.0.0-beta.45",
    "@babel/preset-typescript": "^7.0.0-beta.45",
    "@babel/register": "^7.0.0-beta.45",
    "@types/cookie-parser": "^1.4.1",
    "@types/jasmine": "^2.8.6",
    "@types/knex": "^0.14.12",
    "@types/morgan": "^1.7.35",
    "@types/serve-favicon": "^2.2.30",
    "@types/uuid": "^3.4.3",
    "concurrently": "^3.5.0",
    "knex": "^0.14.6",
    "nodemon": "^1.12.1",
    "path": "^0.12.7",
    "serve-favicon": "^2.4.5",
    "tsc-watch": "^1.0.21",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "typescript-register": "^1.1.0"
  }
}
