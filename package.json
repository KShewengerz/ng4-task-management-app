{
  "name": "task-management-system",
  "version": "0.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "build": "concurrently \"npm install\" \"cd public/app && npm install\"",
    "ng-start": "cd ./public/app && npm start",
    "express-start": "nodemon ./bin/www --exec babel-node",
    "start": "concurrently \"npm run express-start\"  \"npm run ng-start\""
  },
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^7.0.60",
    "babel-cli": "^6.26.0",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2",
    "typescript": "^2.8.1"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.8",
    "@types/cookie-parser": "^1.4.1",
    "@types/jasmine": "^2.8.6",
    "@types/morgan": "^1.7.35",
    "@types/serve-favicon": "^2.2.30",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.0",
    "nodemon": "^1.12.1",
    "path": "^0.12.7",
    "serve-favicon": "^2.4.5",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
